<?xml version="1.0" encoding="utf-8"?>
<TableRow
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/headerSum"
                            android:text="Razem"
                        android:textStyle="bold"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <LinearLayout
        android:id="@+id/headerImmediatelyPoints"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/headerImmediatelyPointsPalaceDesigners"
            android:src="@drawable/PalaceDesigner"
            android:layout_width="50dp"
            android:layout_height="50dp"/>
        <ImageView
            android:id="@+id/headerImmediatelyPointsGatesWithoutEnd"
            android:src="@drawable/GatesPoints"
            android:layout_width="50dp"
            android:layout_height="50dp"/>
    </LinearLayout>
    <ImageView
        android:id="@+id/headerWalls"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="150dp"
        android:src="@drawable/Walls"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:id="@+id/headerPavilion"
        android:layout_width="@dimen/game_details_cell_width"
        android:src="@drawable/Pavilion"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:id="@+id/headerSeraglio"
        android:layout_width="@dimen/game_details_cell_width"
        android:src="@drawable/Seraglio"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:id="@+id/headerArcades"
        android:layout_width="@dimen/game_details_cell_width"
        android:src="@drawable/Arcades"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:id="@+id/headerChambers"
        android:layout_width="@dimen/game_details_cell_width"
        android:src="@drawable/Chambers"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:id="@+id/headerGarden"
        android:layout_width="@dimen/game_details_cell_width"
        android:src="@drawable/Garden"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:id="@+id/headerTower"
        android:layout_width="@dimen/game_details_cell_width"
        android:src="@drawable/Tower"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <LinearLayout
        android:id="@+id/headerBonuses"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/headerBonusesBonusCards"
            android:src="@drawable/BonusCard"
            android:layout_width="50dp"
            android:layout_height="50dp"/>
        <ImageView
            android:id="@+id/headerBonusesExtensions"
            android:src="@drawable/Extension"
            android:layout_width="50dp"
            android:layout_height="50dp"/>
        <ImageView
            android:id="@+id/headerBonusesGatesWithoutEnd"
            android:src="@drawable/SemiBuilding"
            android:layout_width="50dp"
            android:layout_height="50dp"/>
    </LinearLayout>
    <ImageView
        android:src="@drawable/Servant"
        android:id="@+id/headerBuildingsWithoutServantTile"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:src="@drawable/Fruit"
        android:id="@+id/headerOrchards"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
