        android:layout_marginStart="10dp"/>
    <ImageView
        android:src="@drawable/Mission7"
        android:id="@+id/headerMission7"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
    <ImageView
        android:src="@drawable/Mission8"
        android:id="@+id/headerMission8"
        android:layout_width="@dimen/game_details_cell_width"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"/>
